{
  "manifest_version": 2,

  "name": "ABC Automatic Bookmark Classifier",
  "description": "Categorize bookmarks with machine learning",
    "version": "1.0.0",
    "icons": {
        "128": "128.png"
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
  "permissions": [
    "tabs",
    "<all_urls>",
    "bookmarks"
  ]
}